{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Mplus",
      "type": "process",
      "command": "mplus.exe",
      "args": ["${fileBasename}", "/diagram"],
      "options": {
        "cwd": "${fileDirname}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Open Mplus Diagrammer",
      "type": "process",
      "command": "Diagrammer.exe",
      "args": ["${fileBasename}"],
      "options": {
        "cwd": "${fileDirname}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
